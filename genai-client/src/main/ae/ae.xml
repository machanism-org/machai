<Environment xmlns="http://ganteater.com/xml/configuration"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ganteater.com/xml/configuration https://ganteater.com/xml/configuration/anteater-1.2.4.2.xsd">

	<Configuration name="#Base">
		<Var name="WEBDRIVER_PATH" value="." />
		<Recipes path="recipes" />
		<Menu name="Recipes">
			<item task="Submit Files" />
			<item task="Submit Prompt" />
		</Menu>
	</Configuration>

	<Configuration name="CodeMie" base="#Base">
		<Var name="PROMPT_RECIPE_NAME" value="Prompt To CodeMie" />
		<Startup task="Run CodeMie Plugin" async="true" />
		<Startup task="Open CodeMie" />
	</Configuration>

	<Configuration name="AIDial" base="#Base">
		<Var name="PROMPT_RECIPE_NAME" value="Prompt To AIDial" />
		<Startup task="Open AIDial" />
	</Configuration>

</Environment>
